@import './weui-miniprogram/weui-wxss/dist/style/weui.wxss';
page{
  background: #F2F2F2;
}
view {
  box-sizing: border-box;
}
text,input,textarea{
  font-size: 14px;
}
.text_one{
  white-space:nowrap; 
  overflow:hidden; 
  text-overflow:ellipsis;
}

.text_two{
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
}

button[type="def"]{
  background:#FCD055;
}

button[type="text"]{
  position: relative;
  display: block;
  margin: 0!important;
  padding: 0!important;
  box-sizing: border-box;
  font-size: 16px;
  text-align: center;
  text-decoration: none;
  line-height: 2.55555556;
  border-radius: 0!important;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: #000;
  background-color: transparent!important;
}
button[type="text"]::after{
  border: none;
}